{
  "include": ["src/**/*.ts"],
  "compilerOptions": {
    "outDir": "./dist/",
    "module": "Node16",
    "target": "ES6",
    "moduleResolution": "Node16",
    "lib": ["ESNext"],
    "alwaysStrict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}
